package ml.bosstop.deathrun;

import org.bukkit.ChatColor;
// import org.bukkit.event.Listener;
import org.bukkit.plugin.java.JavaPlugin;
import ml.bosstop.deathrun.commands.commands;

public class Main extends JavaPlugin {
	
	public String prefix() {
		return "&f[&4DeathRun&f] "; 
	}
	
    @Override
    public void onEnable() {
    	
    	new commands(this);
    	
    	System.out.println(ChatColor.translateAlternateColorCodes('&', prefix() + "&fStarting DeathRun v1.0"));
    	System.out.println(ChatColor.translateAlternateColorCodes('&', prefix() + "&fDeathRun Active"));
    }
    
    @Override
    public void onDisable() {
    	
    	System.out.println(ChatColor.translateAlternateColorCodes('&', prefix() + "&fDisabling DeathRun v1.0"));
    	System.out.println(ChatColor.translateAlternateColorCodes('&', prefix() + "&fDeathRun Disabled"));

    }
}