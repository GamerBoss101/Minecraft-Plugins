package ml.bosstop.deathrun;

import org.bukkit.ChatColor;
// import org.bukkit.event.Listener;
import org.bukkit.plugin.java.JavaPlugin;
import ml.bosstop.deathrun.commands.commands;
import ml.bosstop.deathrun.util.Util;


public class Main extends JavaPlugin {	
	
    @Override
    public void onEnable() {
    	
    	new commands(this);
    	
    	System.out.println(ChatColor.translateAlternateColorCodes('&', Util.prefix() + "&fStarting DeathRun v1.0"));
    	System.out.println(ChatColor.translateAlternateColorCodes('&', Util.prefix() + "&fDeathRun Active"));
    }

	@Override
    public void onDisable() {
    	
    	System.out.println(ChatColor.translateAlternateColorCodes('&', Util.prefix() + "&fDisabling DeathRun v1.0"));
    	System.out.println(ChatColor.translateAlternateColorCodes('&', Util.prefix() + "&fDeathRun Disabled"));

    }
}